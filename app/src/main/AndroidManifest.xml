<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liuting.jetpack.jetpackdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".MainAppAplication"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main2" />
        <activity android:name=".login.ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.notifications.XRecyclerViewActivity"/>
        <activity
            android:name=".ui.notifications.xrecyclerview.AlphaChangeActivity"
            android:label="@string/app_name"
            android:theme="@style/SupportTheme" />

        <activity
            android:name=".ui.notifications.xrecyclerview.LinearActivity"
            android:label="@string/app_name"
            android:theme="@style/SupportTheme" />
        <activity
            android:name=".ui.notifications.xrecyclerview.GridActivity"
            android:label="@string/app_name"
            android:theme="@style/SupportTheme" />
        <activity
            android:name=".ui.notifications.xrecyclerview.StaggeredGridActivity"
            android:label="@string/app_name"
            android:theme="@style/SupportTheme" />

        <activity
            android:name=".ui.notifications.xrecyclerview.LinearStickyScrollActivity"
            android:label="@string/app_name"
            android:theme="@style/SupportTheme"/>

        <activity
            android:name=".ui.notifications.xrecyclerview.EmptyViewActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ui.notifications.xrecyclerview.CollapsingToolbarLayoutActivity"
            android:theme="@style/SupportTheme" />
        <activity
            android:name=".ui.notifications.xrecyclerview.DisableExampleActivity"
            android:theme="@style/SupportTheme" />
        <activity
            android:name=".ui.notifications.xrecyclerview.MultiHeaderActivity"
            android:theme="@style/SupportTheme" />
        <activity
            android:name=".ui.notifications.xrecyclerview.ItemDecorationActivity"
            android:theme="@style/SupportTheme"/>

    </application>

</manifest>